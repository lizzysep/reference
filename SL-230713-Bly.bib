@Article{Grinberg04p220101,
  author    = {Grinberg, I. and Rappe, A. M.},
  journal   = {Phys. Rev. B},
  title     = {Local structure and macroscopic properties in PbMg$_{1/3}$Nb$_{2/3}$O$_3$--PbTiO$_3$ and PbZn$_{1/3}$Nb$_{2/3}$O$_3$--PbTiO$_3$ solid solutions},
  year      = {2004},
  issn      = {0163-1829},
  number    = {22},
  pages     = {220101-1-4},
  volume    = {70},
  abstract  = {We have examined the local structure of PbMg/sub 1/3/Nb/sub 2/3/O/sub 3/-PbTiO/sub 3/ (PMN-PT) and PbZn/sub 1/3/Nb/sub 2/3/O/sub 3/-PbTiO/sub 3/ (PZN-PT) solid solutions using density functional theory. We find that the directions and magnitudes of cation displacement can be explained by an interplay of cation-oxygen bonding, electrostatic dipole-dipole interactions, and short-range direct and through oxygen Pb-B-cation repulsive interactions. We find that the Zn ions move off center in the PZN-PT system, which also enables larger Pb and Nb/Ti displacements. The off-centering behavior of Zn lessens Pb-B-cation repulsion, leading to relaxor-to-ferroelectric and rhombohedral-to-tetragonal phase transitions at low PbTiO/sub 3/ content in the PZN-PT system. We also show that a simple quadratic relationship exists between Pb and B-cation displacements and the temperature maximum of the dielectric constant, thus linking the enhanced displacements in PZN-PT systems with the higher transition temperatures},
  copyright = {Copyright 2005, IEE},
  doi       = {10.1103/PhysRevB.70.220101},
  keywords  = {bonds (chemical);density functional theory;ferroelectric transitions;lead compounds;permittivity;relaxor ferroelectrics;solid solutions;},
  language  = {English},
}

