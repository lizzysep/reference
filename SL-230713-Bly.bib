@Article{Shin05p054104,
  author    = {Y.-H. Shin and Cooper, V. R. and Grinberg, I. and Rappe, A. M.},
  journal   = {Phys. Rev. B},
  title     = {Development of a bond-valence molecular-dynamics model for complex oxides},
  year      = {2005},
  issn      = {0163-1829},
  number    = {5},
  pages     = {054104-1--4},
  volume    = {71},
  abstract  = {A simple ten-parameter interatomic potential model is described that is capable of accurately reproducing the static and dynamical properties of complex oxides. The accuracy of this model stems from the crystal-chemical bond-valence theory of ionic and covalent bonding. The development of a specific variant of this model for ferroelectric PbTiO/sub 3/ (PT) is discussed in detail, and comparison of the model is made with density functional theory computations and with experimental data. Bond-valence molecular dynamics (BVMD) simulations for PT show a ferroelectric transition at 575 K. The BVMD model correctly reproduces the mixed order-disorder and displacive phase transition character, the magnitudes of cation displacements in the ferroelectric and paraelectric phases, and the energy of 180 degrees domain walls. The success of this simple and physically motivated model makes the simulation of extended defects tractable in PT and other complex oxides},
  copyright = {Copyright 2005, IEE},
  doi       = {10.1103/PhysRevB.71.054104},
  keywords  = {bonds (chemical);electric domain walls;ferroelectric materials;ferroelectric transitions;molecular dynamics method;order-disorder transformations;VB calculations;},
