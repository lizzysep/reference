@Article{Snaith14p1511,
  author   = {Snaith, Henry J. and Abate, Antonio and Ball, James M. and Eperon, Giles E. and Leijtens, Tomas and Noel, Nakita K. and Stranks, Samuel D. and Wang, Jacob Tse-Wei and Wojciechowski, Konrad and Zhang, Wei},
