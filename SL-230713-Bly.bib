@Article{Xiao15p193,
  author    = {Xiao, Zhengguo and Yuan, Yongbo and Shao, Yuchuan and Wang, Qi and Dong, Qingfeng and Bi, Cheng and Sharma, Pankaj and Gruverman, Alexei and Huang, Jinsong},
  journal   = {Nat. Mater.},
  title     = {Giant Switchable Photovoltaic Effect in Organometal Trihalide Perovskite Devices},
  year      = {2015},
  issn      = {1476-1122},
  month     = feb,
  number    = {2},
  pages     = {193--198},
  volume    = {14},
  abstract  = {Organolead trihalide perovskite (OTP) materials are emerging as naturally abundant materials for low-cost, solution-processed and highly efficient solar cells. Here, we show that, in OTP-based photovoltaic devices with vertical and lateral cell configurations, the photocurrent direction can be switched repeatedly by applying a small electric field of {\textless}1 V ??m???1. The switchable photocurrent, generally observed in devices based on ferroelectric materials, reached 20.1 mA cm???2 under one sun illumination in OTP devices with a vertical architecture, which is four orders of magnitude larger than that measured in other ferroelectric photovoltaic devices. This field-switchable photovoltaic effect can be explained by the formation of reversible p???i???n structures induced by ion drift in the perovskite layer. The demonstration of switchable OTP photovoltaics and electric-field-manipulated doping paves the way for innovative solar cell designs and for the exploitation of OTP materials in electrically and optically readable memristors and circuits.},
  copyright = {? 2014 Nature Publishing Group},
  doi       = {10.1038/nmat4150},
  file      = {Full Text PDF:/Users/shimac/Library/Application Support/Zotero/Profiles/hq5nznv3.default/zotero/storage/8Z2NTGQK/Xiao et al. - 2015 - Giant switchable photovoltaic effect in organometa.pdf:application/pdf;Snapshot:/Users/shimac/Library/Application Support/Zotero/Profiles/hq5nznv3.default/zotero/storage/K8IV7638/nmat4150.html:text/html},
  keywords  = {ion diffusion},
  language  = {en},
  url       = {http://www.nature.com/nmat/journal/v14/n2/abs/nmat4150.html},
  urldate   = {2015-05-07},
}

